		div#loginModal.modal.fade
			.modal-dialog
				.modal-content
					.modal-header
						button(type='button',class='close',data-dismiss='modal') &times;
						h4.modal-title Login to your account
					.modal-body
						form
							.form-group
								label(for='username') Username
								input.form-control#username(type="text")
							.form-group
								label(for='username') Password
								input.form-control#psw(type="password")
							.checkbox
								label #[input(type="checkbox")] Remember me
								a.forgot-link(href="#" data-toggle='modal', data-target='#RememberPsw') Forgot your password?
								button(type='submit').btn.btn-info.btn-lg.btn-block LOGIN
						p Dont have an account yet? #[a(href="#") Create an account]
		#rememberPsw.modal.fade
			.modal-dialog
				.modal-content
					.modal-header
						button(type='button',class='close',data-dismiss='modal') &times;
						h4.modal-title Forgot Password?
					.modal-body
						form
							.form-group
								label(for='username') Email Address
								input.form-control#username(type="email")
							.checkbox
								button(type='submit').btn.btn-info.btn-lg.btn-block SEND RESET INSTRUCTION
