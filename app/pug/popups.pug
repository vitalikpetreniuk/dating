		#upload.modal.fade
			.modal-dialog.modal-lg
				.modal-content
					.modal-header.red
						button(type='button',class='close',data-dismiss='modal') &times;
						h4.modal-title Would you like to upload a public or private Profile Picture?
					.modal-body.red
						.three-ways
							.block-desc 3 ways to keep your profile anonymous
							.way
								.desc 
									.text #[span.number 1.] Use our #[i Photoblur] feature to blur out your face.
								.photos
									img(src='img/blur-face1.jpg')
									img(src='img/blur-face2.jpg')
							.way
								.desc 
									.text  #[span.number 2.] Upload private photos that only people you choose can see.
								.photos
									img(src='img/locked-face1.jpg')
									img(src='img/locked-face2.jpg')
							.way
								.desc 
									.text #[span.number 3.] Only show bodyparts
								.photos
									img(src='img/body-face1.jpg')
									img(src='img/body-face2.jpg')
						.info
							.buttons
								button.btn.btn-info.btn-lg.btn-block Add a photo
								button.btn.btn-info.btn-lg.btn-block Don't add a photo
		#edit.modal.fade
					.modal-dialog.modal-md
						.modal-content
							.modal-header.red
								button(type='button',class='close',data-dismiss='modal') &times;
								h4.modal-title Edit Photo
							.modal-body.red
								.info-message Click on any of the buttons below to edit your photo. Once complete, click #[span.underline SAVE]
								#blurSlider
								.photo
									img(src="img/edit_photo.jpg", alt="girl")
								.buttons.top
									button.btn.btn-info.btn-lg.blur Blur
									button.btn.btn-info.btn-lg.crop Crop
									button.btn.btn-info.btn-lg Make Private
								button.btn.btn-info.btn-lg.done-btn ADD MORE
								button.btn.btn-info.btn-lg.save-btn Save

